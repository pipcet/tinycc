tcc binary: example-interpreter-in-usr-local usr/share/doc/tcc/examples/ex1.c #!/usr/local/bin/tcc
tcc binary: example-interpreter-in-usr-local usr/share/doc/tcc/examples/ex4.c #!/usr/local/bin/tcc
